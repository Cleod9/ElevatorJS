package com.mcleodgaming.elevator.events{	public class Event	{		//This is a generic Event class for use with EventDispatcher. It does nothing but contain a type identifier, and hold some data		public var type:String;		public var data:Object;		public function Event(typeVal:String, dataVal:Object):void {			this.type = typeVal || "event";			this.data = dataVal || {};		}	}}