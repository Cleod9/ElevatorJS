package com.mcleodgaming.elevator.core{	import com.mcleodgaming.elevator.core.*;		public class Floor {		//A Floor contains a list of Person objects		public var index:int;		public var name:String; //Name for the floor (e.g. "Floor 1")		public var people:Array;		public function Floor(params:Object):void {			//Get settings			params = params || {};			index = (typeof params.index == "number") ? params.index : -1;			name = params.name || "[No Name]";			people = new Array();		}		public function removePerson(person:Person):void {			//Remove a person from the floor			var i = people.indexOf(person);			if(i >= 0)				people.splice(i, 1)		}		public function addPerson(person:Person):void {			//Add a person to the floor			if(people.indexOf(person) < 0)				people.push(person);		}	}};